{
    "initial_working_directory": "~",
    "editor_theme": "Dracula",
    "posix_terminal_shell": "bash",
    "save_workspace": "never",
    "panes": {
        "quadrants": [
            "Source",
            "TabSet1",
            "Console",
            "TabSet2"
        ],
        "tabSet1": [
            "Presentation"
        ],
        "tabSet2": [
            "Environment",
            "Plots",
            "Packages",
            "Help",
            "Presentations"
        ],
        "hiddenTabSet": [
            "History",
            "Files",
            "Connections",
            "Build",
            "VCS",
            "Tutorial",
            "Viewer",
            "Presentations"
        ],
        "console_left_on_top": true,
        "console_right_on_top": true,
        "additional_source_columns": 0
    },
    "spelling_dictionary_language": "en_GB",
    "soft_wrap_r_files": true,
    "font_size_points": 12,
    "syntax_color_console": true,
    "jobs_tab_visibility": "shown",
    "editor_keybindings": "vim",
    "source_with_echo": true,
    "rainbow_parentheses": true,
    "indent_guides": "rainbowlines"
}